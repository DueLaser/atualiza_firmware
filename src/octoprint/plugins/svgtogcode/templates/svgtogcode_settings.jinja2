<div id="settings_plugin_svgtogcode_dialog" xdata-bind="allowBindings: true">
    <h4>{{ _('Configurações Padrão') }}</h4>

    <form class="form-horizontal">
        <div class="control-group">
            <label class="control-label" for="settings-svgtogcode-defaultIntensity">{{ _('Potência') }}</label>
            <div class="controls">
                <input type="number" min="1" max="1000" class="input-block-level" data-bind="event: {change: saveall}, value: settings.plugins.svgtogcode.defaultIntensity">
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="settings-svgtogcode-defaultFeedrate">{{ _('Velocidade') }}</label>
            <div class="controls">
                <input type="number" min="250" class="input-block-level" data-bind="event: {change: saveall}, value: settings.plugins.svgtogcode.defaultFeedrate">
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="settings-svgtogcode-svgDPI">{{ _('SVG dpi') }}</label>
            <div class="controls">
                <input type="text" class="input-block-level" data-bind="event: {change: saveall}, value: settings.plugins.svgtogcode.svgDPI">
            </div>
        </div>
    </form>
</div>